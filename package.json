{
  "name": "availity-uikit",
  "description": "Availity UI Kit powered by Bootstrap 3",
  "version": "2.0.2",
  "author": "Robert McGuinness <rob.mcguinness@availity.com>",
  "engines": {
    "node": ">= 5.0.0",
    "npm": ">= 4.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node cli.js start",
    "build:lib": "babel js --out-dir lib --quiet --ignore spec.js",
    "build:umd": "cross-env NODE_ENV=production node cli.js build",
    "build": "npm run build:umd && npm run build:lib",
    "clean": "node cli.js clean",
    "docs": "npm run docs:publish",
    "docs:pages": "cross-env NODE_ENV=development node cli.js docs",
    "docs:publish": "npm run clean && npm run docs:pages && gh-pages -d build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm run build",
    "release": "node cli.js release",
    "lint": "node cli.js lint"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "style": "dist/css/availity-uikit.css",
  "less": "lib/less/bootstrap.less",
  "homepage": "http://availity.github.io/availity-uikit/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Availity/availity-uikit.git"
  },
